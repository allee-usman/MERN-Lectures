<%- include('partials/header') %>

<h1>Home</h1>
<p><%= homeContent %></p>
<% posts.forEach(post => { %>
<h1><%= post.postTitle %></h1>
<div>
	<p style="display: inline-block">
		<%= (post.postContent.substring(0, 100)) + '....' %>
	</p>
	<a href="/posts/<%= post.postTitle %>">Read more</a>
</div>

<% }); %> <%- include('partials/footer') %>
